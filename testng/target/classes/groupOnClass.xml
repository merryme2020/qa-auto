<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="suitname">

    <test name="runAll">
        <classes>
            <class name="com.yizhan.testng.groups.GroupOnClass1"/>
            <class name="com.yizhan.testng.groups.GroupOnClass2"/>
            <class name="com.yizhan.testng.groups.GroupOnClass3"/>
        </classes>

    </test>

    <test name="onlyRunStu">
        <groups>
            <run>
                <include name="stu"/>
            </run>
        </groups>
        <classes>
            <class name="com.yizhan.testng.groups.GroupOnClass1"/>
            <class name="com.yizhan.testng.groups.GroupOnClass2"/>
            <class name="com.yizhan.testng.groups.GroupOnClass3"/>
        </classes>



    </test>


</suite>